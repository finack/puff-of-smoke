{
  "name": "pos",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "npx @biomejs/biome lint ./app",
    "lint:fix": "npx @biomejs/biome lint --apply ./app",
    "pretty": "npx @biomejs/biome format ./app",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx ./app/db/migrate.server.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx ./app/db/seed.server.ts",
    "pretty:fix": "npx @biomejs/biome format --write ./app",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@headlessui/react": "^2.0.0-alpha.4",
    "@heroicons/react": "^2.1.3",
    "@remix-run/node": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "@remix-run/serve": "^2.9.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.10",
    "envalid": "^8.0.0",
    "isbot": "^5.1.6",
    "pg": "^8.11.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@remix-run/dev": "^2.9.1",
    "@tailwindcss/forms": "^0.5.7",
    "@types/better-sqlite3": "^7.6.10",
    "@types/pg": "^8.11.6",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "csv": "^6.3.8",
    "csv-parse": "^5.5.5",
    "drizzle-kit": "^0.21.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
